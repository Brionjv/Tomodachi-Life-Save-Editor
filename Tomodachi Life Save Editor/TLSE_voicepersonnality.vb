        Savefileregion()
        Readvoicepersonnality()
    End Sub

    Public Sub Readvoicepersonnality()
        Try
            Dim Readdata As New PackageIO.Reader(savedataArc, PackageIO.Endian.Little)
            Readdata.Position = Energy
            valu_energy.Value = Readdata.ReadByte
            Readdata.Position = Speech
            valu_speech.Value = Readdata.ReadByte
            Readdata.Position = Facialexpressions
            valu_facialexpressions.Value = Readdata.ReadByte
            Readdata.Position = Mood
            valu_mood.Value = Readdata.ReadByte
            Readdata.Position = Overall
            valu_overall.Value = Readdata.ReadByte
            Readdata.Position = Pitch
            valu_pitch.Value = Readdata.ReadByte
            Readdata.Position = Speed
            valu_speed.Value = Readdata.ReadByte
            Readdata.Position = Quality
            valu_quality.Value = Readdata.ReadByte
            Readdata.Position = Tone
            valu_tone.Value = Readdata.ReadByte
            Readdata.Position = Accent
            valu_accent.Value = Readdata.ReadByte
            Readdata.Position = Intonation
            valu_intonation.Value = Readdata.ReadByte
        Catch ex As Exception
            TLSE_dialog.Text_TLSE_dialog.Text = "Failed to read voice and personnality"
            TLSE_dialog.ShowDialog()
        End Try
    End Sub

    Private Sub Text_menu_save_Click(sender As Object, e As EventArgs) Handles Text_menu_save.Click
        Writevoicepersonnality()
        TLSE_hub.Show()
        TLSE_hub.Filever_text.Text = Filever_text.Text
        TLSE_hub.TLSE_filepath.Text = TLSE_filepath.Text
        TLSE_hub.TLSE_menu.Visible = False
        TLSE_hub.TLSE_menu_Miiedition.Visible = True
        Me.Close()
    End Sub

    Public Sub Writevoicepersonnality()
        Try
            Dim Writedata As New PackageIO.Writer(savedataArc, PackageIO.Endian.Little)
            Writedata.Position = Energy
            Writedata.WriteInt8(valu_energy.Value)
            Writedata.Position = Speech
            Writedata.WriteInt8(valu_speech.Value)
            Writedata.Position = Facialexpressions
            Writedata.WriteInt8(valu_facialexpressions.Value)
            Writedata.Position = Mood
            Writedata.WriteInt8(valu_mood.Value)
            Writedata.Position = Overall
            Writedata.WriteInt8(valu_overall.Value)
            Writedata.Position = Pitch
            Writedata.WriteInt8(valu_pitch.Value)
            Writedata.Position = Speed
            Writedata.WriteInt8(valu_speed.Value)
            Writedata.Position = Quality
            Writedata.WriteInt8(valu_quality.Value)
            Writedata.Position = Tone
            Writedata.WriteInt8(valu_tone.Value)
            Writedata.Position = Accent
            Writedata.WriteInt8(valu_accent.Value)
            Writedata.Position = Intonation
            Writedata.WriteInt8(valu_intonation.Value)
            TLSE_dialog.Text_TLSE_dialog.Text = "Voice and personnality has been successfully edited"
            TLSE_dialog.ShowDialog()
        Catch ex As Exception
            TLSE_dialog.Text_TLSE_dialog.Text = "Failed to edit Voice and personnality"
            TLSE_dialog.ShowDialog()
        End Try
    End Sub
