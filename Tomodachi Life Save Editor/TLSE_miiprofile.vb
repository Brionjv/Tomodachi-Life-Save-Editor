    Public Sub Readmiiprofile()
        Try
            Dim Readdata As New PackageIO.Reader(savedataArc, PackageIO.Endian.Little)
            Readdata.Position = firstname
            Text_firstname.Text = Readdata.ReadUnicodeString(15)
            Readdata.Position = lastname
            Text_lastname.Text = Readdata.ReadUnicodeString(15)
            Readdata.Position = nickname
            Text_nickname.Text = Readdata.ReadUnicodeString(10)
            If Filever_text.Text = "EU" Or Filever_text.Text = "US" Or Filever_text.Text = "KR" Then
                Readdata.Position = pronunnickname
                Text_pronun_nickname.Text = Readdata.ReadUnicodeString(20)
                Readdata.Position = pronunfirstname
                Text_pronun_firstname.Text = Readdata.ReadUnicodeString(30)
                Readdata.Position = pronunlastname
                Text_pronun_lastname.Text = Readdata.ReadUnicodeString(30)
            End If
            Readdata.Position = creator
            Text_creator.Text = Readdata.ReadUnicodeString(10)
            Readdata.Position = sharing
            valu_sharing.Value = Readdata.ReadByte
            Readdata.Position = copying
            valu_copying.Value = Readdata.ReadByte
            Readdata.Position = gender
            valu_gender.Value = Readdata.ReadByte
            Readdata.Position = favcolor
            valu_favcolor.Value = Readdata.ReadByte
            Readdata.Position = relationyou
            valu_relationyou.Value = Readdata.ReadByte
            Readdata.Position = growkid
            valu_growkid.Value = Readdata.ReadByte
            Readdata.Position = miisysid
            Text_MiisysID.Text = Readdata.ReadHexString(8)
            Readdata.Position = TLmiisysid
            Text_TLMiisysID.Text = Readdata.ReadHexString(8)
            Readdata.Position = originisland
            Text_originisld.Text = Readdata.ReadHexString(16)
            Readdata.Position = actualisland
            Text_actualIsld.Text = Readdata.ReadHexString(16)
        Catch ex As Exception
            TLSE_dialog.Text_TLSE_dialog.Text = "Failed to read Mii profile"
            TLSE_dialog.ShowDialog()
        End Try
    End Sub

